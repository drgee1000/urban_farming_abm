<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>

		<parameter name="UseAreaFarm" displayName="use area to farm" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>

				<parameter name="UseAreaSupermarket" displayName="use area to supermarket" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>

				<parameter name="FARM_NUM" displayName="Farm Num" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>

				<parameter name="SUPERMARKET_NUM" displayName="Supermarket Num" type="int" 
								defaultValue="100" 
								isReadOnly="false" 
								converter="repast.simphony.parameter.StringConverterFactory$IntConverter"

				/>

		
				<parameter name="AGENT_NUM" displayName="Agent Num" type="int" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


